		"property_catalog_t": "currency 货币 AUD BGN BRL CAD CHF CNY CZK DKK EUR GBP HKD HRK HUF IDR ILS INR ISK JPY KRW MXN MYR NOK NZD PHP PLN RON RUB SEK SGD THB TRY USD ZAR",
		"html_s": "<table id='currency'><tr><th>貨幣(Currency)</th><th>牌價(Rate)</th></tr></table>\n\t\t<script>\n\t\t\t(function () {\n\t\t\t\tvar table = document.getElementById('currency');\n\t\t\t\tvar request = new XMLHttpRequest();\n\t\t\t\trequest.onload = function (e) {\n\t\t\t\t\tvar rates = request.response['rates'];\n\t\t\t\t\tvar keys = Object.keys(rates).sort();\n\t\t\t\t\tfor (var rate in keys) {\n\t\t\t\t\t\tvar row = document.createElement('tr');\n\t\t\t\t\t\tvar key = document.createElement('td');\n\t\t\t\t\t\tkey.textContent = keys[rate];\n\t\t\t\t\t\trow.appendChild(key);\n\t\t\t\t\t\tvar value = document.createElement('td');\n\t\t\t\t\t\tvalue.textContent = 1.0 / rates[keys[rate]];\n\t\t\t\t\t\trow.appendChild(value);\n\t\t\t\t\t\ttable.appendChild(row);\n\t\t\t\t\t}\n\t\t\t\t};\n\t\t\t\trequest.open('GET', 'https://api.exchangeratesapi.io/latest?base=HKD');\n\t\t\t\trequest.responseType = 'json';\n\t\t\t\trequest.send();\n\t\t\t})();\n\t\t</script>"
	}
