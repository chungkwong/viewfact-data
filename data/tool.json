		"property_title_t": "顔色|Color Picker",
		"property_catalog_t": "RGB HSL",
		"html_s": "<label for='color-in-r'>R</label><input type='number' value='255' min='0' max='255' step='1' id='color-in-r'>\n<label for='color-in-g'>G</label><input type='number' value='0' min='0' max='255' step='1' id='color-in-g'>\n<label for='color-in-b'>B</label><input type='number' value='0' min='0' max='255' step='1' id='color-in-b'>\n<br>\n<label for='color-in-h'>H</label><input type='number' value='0' step='1' id='color-in-h'>\n<label for='color-in-s'>S</label><input type='number' value='100' min='0' max='100' step='1' id='color-in-s'>\n<label for='color-in-l'>L</label><input type='number' value='100' min='0' max='100' step='1' id='color-in-l'>\n<div id='color-out' style='width:100%'></div>\n<script>\n\tvar colorR=document.getElementById('color-in-r');\n\tvar colorG=document.getElementById('color-in-g');\n\tvar colorB=document.getElementById('color-in-b');\n\tvar colorH=document.getElementById('color-in-h');\n\tvar colorS=document.getElementById('color-in-s');\n\tvar colorL=document.getElementById('color-in-l');\n\tvar colorOut=document.getElementById('color-out');\n\tvar colorHueToRgb=function(m1,m2,h){\n\t\tif(h<0){\n\t\t\th+=1;\n\t\t}else if(h>1){\n\t\t\th-=1;\n\t\t}\n\t\tif(h*6<1){\n\t\t\treturn m1+(m2-m1)*h*6;\n\t\t}else if(h*2<1){\n\t\t\treturn m2;\n\t\t}else if(h*3<2){\n\t\t\treturn m1+(m2-m1)*(2/3-h)*6;\n\t\t}else{\n\t\t\treturn m1;\n\t\t}\n\t};\n\tvar colorHslToRgb=function(h,s,l){\n\t\tvar m2=l<=0.5?l*(s+1):l+s-l*s;\n\t\tvar m1=l*2-m2;\n\t\tvar r=Math.floor(colorHueToRgb(m1,m2,h+1/3)*255);\n\t\tvar g=Math.floor(colorHueToRgb(m1,m2,h)*255);\n\t\tvar b=Math.floor(colorHueToRgb(m1,m2,h-1/3)*255);\n\t\treturn [Math.floor(r),Math.floor(g),Math.floor(b)];\n\t};\n\tvar colorRgbToHsl=function(r,g,b){\n\t\tvar max=Math.max(r,Math.max(g,b));\n\t\tvar min=Math.min(r,Math.min(g,b));\n\t\tvar h;\n\t\tif(max==min){\n\t\t\th=0;\n\t\t}else if(max==r){\n\t\t\th=60*(0+(g-b)/(max-min));\n\t\t}else if(max==g){\n\t\t\th=60*(2+(b-r)/(max-min));\n\t\t}else if(max==b){\n\t\t\th=60*(4+(r-g)/(max-min));\n\t\t}\n\t\tvar l=(max+min)*100/2/255;\n\t\tvar s=max==0||min==255?0:(max-min)*100/255/(1-Math.abs(2*l/100-1));\n\t\treturn [Math.floor(h),Math.floor(s),Math.floor(l)];\n\t};\n\tvar colorUpdateHsl=function(){\n\t\tvar h=colorH.value;\n\t\tvar s=colorS.value;\n\t\tvar l=colorL.value;\n\t\tvar rgb=colorHslToRgb((h-Math.floor(h/360)*360)/360,s/100,l/100);\n\t\tcolorR.value=rgb[0];\n\t\tcolorG.value=rgb[1];\n\t\tcolorB.value=rgb[2];\n\t\tvar color='rgb('+rgb[0]+','+rgb[1]+','+rgb[2]+')';\n\t\tcolorOut.style.background=color;\n\t\tcolorOut.textContent=window.getComputedStyle(colorOut).backgroundColor;\n\t};\n\tcolorH.oninput=colorUpdateHsl;\n\tcolorS.oninput=colorUpdateHsl;\n\tcolorL.oninput=colorUpdateHsl;\n\tvar colorUpdateRgb=function(){\n\t\tvar r=colorR.value;\n\t\tvar g=colorG.value;\n\t\tvar b=colorB.value;\n\t\tvar color='rgb('+r+','+g+','+b+')';\n\t\tvar hsl=colorRgbToHsl(r,g,b);\n\t\tcolorH.value=hsl[0];\n\t\tcolorS.value=hsl[1];\n\t\tcolorL.value=hsl[2];\n\t\tcolorOut.style.backgroundColor=color;\n\t\tcolorOut.textContent=window.getComputedStyle(colorOut).backgroundColor;\n\t};\n\tcolorR.oninput=colorUpdateRgb;\n\tcolorG.oninput=colorUpdateRgb;\n\tcolorB.oninput=colorUpdateRgb;\n\tcolorUpdateRgb();\n</script>"
	},
	{
		"property_title_t": "大小寫轉換|Lowercase uppercase converter",
		"property_catalog_t": "",
		"html_s": "<label for='string-lowercase'>Lower case</label><textarea rows='2' style='width:100%' id='string-lowercase'>abc</textarea>\n<label for='string-uppercase'>Upper case</label><textarea rows='2' style='width:100%' id='string-uppercase'>ABC</textarea>\n<script>\n\tvar stringLowercase=document.getElementById('string-lowercase');\n\tvar stringUppercase=document.getElementById('string-uppercase');\n\tstringLowercase.oninput=function(){\n\t\tstringUppercase.value=stringLowercase.value.toLocaleUpperCase();\n\t};\n\tstringUppercase.oninput=function(){\n\t\tstringLowercase.value=stringUppercase.value.toLocaleLowerCase();\n\t};\n</script>"
	},
	{
		"property_title_t": "隨機密碼生成器|Random password generator",
		"property_catalog_t": "",
		"html_s": "<div style='display:flex'>\n\t<label for='password-length'>長度</label><input type='number' min='1' step='1' value='10' id='password-length'>\n\t<label for='password-charset'>可用字符</label><input type='text' value='0123456789abcdefghijklmnopqrstuvwxyzABCDEFGHIJKLMNOPQRSTUVWXYZ' id='password-charset' style='flex: 1'>\n</div>\n<div><output id='password-generated'></output><input type='button' value='重新生成' id='password-refresh'></div>\n<script>\n\tvar passwordLength=document.getElementById('password-length');\n\tvar passwordCharset=document.getElementById('password-charset');\n\tvar passwordGenerated=document.getElementById('password-generated');\n\tvar generatePassword=function(){\n\t\tvar buf='';\n\t\tfor(var i=0;i<passwordLength.value;i++){\n\t\t\tbuf+=passwordCharset.value.charAt(Math.random()*passwordCharset.value.length);\n\t\t}\n\t\tpasswordGenerated.textContent=buf;\n\t};\n\tpasswordLength.oninput=generatePassword;\n\tpasswordCharset.oninput=generatePassword;\n\tdocument.getElementById('password-refresh').onclick=generatePassword;\n\tgeneratePassword();\n</script>"
	},
	{
		"property_title_t": "正則表逹式|Regular expression",
		"property_catalog_t": "regex regexp",
		"html_s": "<div style='display: flex'><label for='regexp-text'>Text</label><textarea id='regexp-text' style='flex:1'></textarea></div>\n<div style='display: flex'><label for='regexp-find'>Regular expression</label><input type='text' value='[a-zA-Z]+' id='regexp-find' style='flex:1'></div>\n<div style='display: flex'><label for='regexp-replacement'>Replacement</label><input type='text' value='*' id='regexp-replacement' style='flex:1'></div>\n<input type='checkbox' id='regexp-ignore-case'><label for='regexp-ignore-case'>Ignore case</label>\n<input type='checkbox' id='regexp-multiline'><label for='regexp-multiline'>Multiline</label>\n<input type='checkbox' id='regexp-unicode'><label for='regexp-unicode'>Unicode</label>\n<div id='regexp-output'></div>\n<script>\n\tvar regexpText=document.getElementById('regexp-text');\n\tvar regexpFind=document.getElementById('regexp-find');\n\tvar regexpReplacement=document.getElementById('regexp-replacement');\n\tvar regexpIgnoreCase=document.getElementById('regexp-ignore-case');\n\tvar regexpMultiline=document.getElementById('regexp-multiline');\n\tvar regexpUnicode=document.getElementById('regexp-unicode');\n\tvar regexpOutput=document.getElementById('regexp-output');\n\tvar regexCompileReplacement=function(replacement){\n\t\tvar struct=[''];\n\t\tvar last=0;\n\t\tvar offset;\n\t\twhile((offset=replacement.indexOf('$',last))!==-1){\n\t\t\tstruct[struct.length-1]+=replacement.substring(last,offset);\n\t\t\tif(offset+1<replacement.length){\n\t\t\t\tvar c=replacement.charAt(++offset);\n\t\t\t\tif(c>='0'&&c<='9'){\n\t\t\t\t\tstruct.push(c-'0');\n\t\t\t\t\tstruct.push('');\n\t\t\t\t}else{\n\t\t\t\t\tstruct[struct.length-1]+=c;\n\t\t\t\t}\n\t\t\t}\n\t\t\tlast=offset+1;\n\t\t}\n\t\tstruct[struct.length-1]+=replacement.substring(last);\n\t\treturn struct;\n\t};\n\tvar regexUpdate=function(){\n\t\tvar text=regexpText.value;\n\t\tvar flags='g';\n\t\tif(regexpIgnoreCase.checked){\n\t\t\tflags+='i';\n\t\t}\n\t\tif(regexpMultiline.checked){\n\t\t\tflags+='m';\n\t\t}\n\t\tif(regexpUnicode.checked){\n\t\t\tflags+='u';\n\t\t}\n\t\ttry{\n\t\t\tvar regexp=new RegExp(regexpFind.value,flags);\n\t\t}catch(e){\n\t\t\tregexpFind.setCustomValidity('Syntax error');\n\t\t\treturn;\n\t\t}\n\t\tregexpFind.setCustomValidity('');\n\t\tvar replacement=regexCompileReplacement(regexpReplacement.value);\n\t\tvar result;\n\t\tvar out='';\n\t\tvar last=0;\n\t\twhile((result=regexp.exec(text))!==null){\n\t\t\tif(result[0].length===0)\n\t\t\t\t++regexp.lastIndex;\n\t\t\tout+=text.substring(last,result.index);\n\t\t\tout+='<em>';\n\t\t\tfor(var i in replacement){\n\t\t\t\tvar component=replacement[i];\n\t\t\t\tif(typeof (component)==='number'){\n\t\t\t\t\tout+=result[component];\n\t\t\t\t}else{\n\t\t\t\t\tout+=component;\n\t\t\t\t}\n\t\t\t}\n\t\t\tout+='</em>';\n\t\t\tlast=result.index+result[0].length;\n\t\t}\n\t\tout+=text.substring(last,text.length);\n\t\tregexpOutput.innerHTML=out;\n\t};\n\tregexpText.oninput=regexUpdate;\n\tregexpFind.oninput=regexUpdate;\n\tregexpReplacement.oninput=regexUpdate;\n\tregexpIgnoreCase.onchange=regexUpdate;\n\tregexpMultiline.onchange=regexUpdate;\n\tregexpUnicode.onchange=regexUpdate;\n</script>"
	},
	{
		"property_title_t": "散列值計算器|Hash calculator",
		"property_catalog_t": "MD5 SHA SHA1 1 SHA3  224 SHA224 SHA256 256 SHA384 SHA3 384 SHA512 512 SHAKE128 SHAKE256",
		"html_s": "<div><label for='security-hash-algorithm'>算法</label><select id='security-hash-algorithm'>\n\t\t<option>MD5</option>\n\t\t<option>SHA-1</option>\n\t\t<option>SHA-224</option>\n\t\t<option>SHA3-224</option>\n\t\t<option>SHA-256</option>\n\t\t<option>SHA3-256</option>\n\t\t<option>SHA-384</option>\n\t\t<option>SHA3-384</option>\n\t\t<option>SHA-512</option>\n\t\t<option>SHA3-512</option>\n\t\t<option>SHAKE128</option>\n\t\t<option>SHAKE256</option>\n\t</select><input id='security-hash-length' type='number' min='8' step='8' value='8' hidden></div>\n<div><label for='security-hash-input'>输入</label><input type='file' id='security-hash-input'></div>\n<div><label for='security-hash-output'>输出</label><output id='security-hash-output' style='overflow-wrap: break-word;'></output></div>\n<script type='text/javascript'>\n\tvar securityLoadedFiles=[];\n\tvar securityEnsureLoaded=function(file,task){\n\t\tif(securityLoadedFiles.indexOf(file)>=0){\n\t\t\ttask();\n\t\t}else{\n\t\t\tvar script=document.createElement('script');\n\t\t\tscript.src=file;\n\t\t\tscript.onload=function(){\n\t\t\t\tsecurityLoadedFiles.push(file);\n\t\t\t\ttask();\n\t\t\t};\n\t\t\tdocument.getElementsByTagName('head')[0].appendChild(script);\n\t\t}\n\t};\n\tvar securityHash=function(data,algorithm,length,callback){\n\t\tif(algorithm==='MD5'){\n\t\t\tsecurityEnsureLoaded('https://assets.viewfact.org/js/md5.js',function(){\n\t\t\t\tcallback(md5(data));\n\t\t\t});\n\t\t}else{\n\t\t\tsecurityEnsureLoaded('https://assets.viewfact.org/js/sha.js',function(){\n\t\t\t\tvar shaObj=new jsSHA(algorithm,'ARRAYBUFFER');\n\t\t\t\tshaObj.update(data);\n\t\t\t\tcallback(shaObj.getHash('HEX',{shakeLen:length}));\n\t\t\t});\n\t\t}\n\t};\n\tvar securityHashAlgorithmChooser=document.getElementById('security-hash-algorithm');\n\tvar securityHashLengthChooser=document.getElementById('security-hash-length');\n\tvar securityHashFileChooser=document.getElementById('security-hash-input');\n\tvar securityHashOutput=document.getElementById('security-hash-output');\n\tvar securityHashRefresh=function(){\n\t\tsecurityHashLengthChooser.hidden=!(securityHashAlgorithmChooser.value.startsWith('SHAKE'));\n\t\tif(securityHashFileChooser.files.length>0){\n\t\t\tvar reader=new FileReader();\n\t\t\tsecurityHashOutput.textContent='請稍候';\n\t\t\treader.onload=function(e){\n\t\t\t\tsecurityHash(e.target.result,securityHashAlgorithmChooser.value,securityHashLengthChooser.value,function(sum){\n\t\t\t\t\tsecurityHashOutput.textContent=sum.toUpperCase();\n\t\t\t\t});\n\t\t\t};\n\t\t\treader.onerror=function(e){\n\t\t\t\tsecurityHashOutput.textContent='讀取文件失敗';\n\t\t\t};\n\t\t\treader.readAsArrayBuffer(securityHashFileChooser.files[0]);\n\t\t}\n\t};\n\tsecurityHashFileChooser.onchange=securityHashRefresh;\n\tsecurityHashAlgorithmChooser.onchange=securityHashRefresh;\n\tsecurityHashLengthChooser.onchange=securityHashRefresh;\n</script>"
	},
	{
		"property_title_t": "時間戳|Timestamp",
		"property_catalog_t": "unix iso 8601",
		"html_s": "<label for='timestamp-number'>Unix timestamp</label><input type='number' step='1' id='timestamp-number'><br>\n<label for='timestamp-iso'>ISO 8601</label><input type='text' id='timestamp-iso'>\n<script>\n\tvar timeStampNumber=document.getElementById('timestamp-number');\n\tvar timeStampIso=document.getElementById('timestamp-iso');\n\tvar timeStampUpdate=function(date){\n\t\ttimeStampNumber.value=date.valueOf();\n\t\ttimeStampIso.value=date.toISOString();\n\t};\n\ttimeStampNumber.oninput=function(){\n\t\ttimeStampUpdate(new Date(parseInt(timeStampNumber.value)));\n\t};\n\ttimeStampIso.oninput=function(){\n\t\ttimeStampUpdate(new Date(timeStampIso.value));\n\t};\n\ttimeStampUpdate(new Date());\n</script>"
	},
	{
		"property_title_t": "字數|Word count",
		"property_catalog_t": "字数统计 lines codepoints words characters",
		"html_s": "<label for='string-count-string'>Text</label><textarea rows='2' style='width:100%' id='string-count-string'>abc</textarea>\n<output id='string-count-character'>3</output><label for='string-count-character'> characters/ </label>\n<output id='string-count-codepoint'>3</output><label for='string-count-codepoint'> codepoints/ </label>\n<output id='string-count-word'>1</output><label for='string-count-word'> words/ </label>\n<output id='string-count-line'>1</output><label for='string-count-line'> lines</label>\n<script>\n\tvar stringCountString=document.getElementById('string-count-string');\n\tvar stringCountChar=document.getElementById('string-count-character');\n\tvar stringCountCode=document.getElementById('string-count-codepoint');\n\tvar stringCountLine=document.getElementById('string-count-line');\n\tvar stringCountWord=document.getElementById('string-count-word');\n\tvar stringUppercase=document.getElementById('string-uppercase');\n\tstringCountString.oninput=function(){\n\t\tvar text=stringCountString.value;\n\t\tstringCountChar.textContent=text.length;\n\t\tstringCountCode.textContent=Array.from(text).length;\n\t\tstringCountLine.textContent=text.split(/\\r|\\n|\\r\\n/).length;\n\t\tvar wordCount=0;\n\t\tvar wordPattern=/\\w+/g;\n\t\twhile(wordPattern.exec(text)){\n\t\t\t++wordCount;\n\t\t}\n\t\tstringCountWord.textContent=wordCount;\n\t};\n</script>"
	}
