		"property_title_t": "URI encoder/decoder",
		"property_catalog_t": "URL escape unescape encode decode",
		"html_s": "<form style=\"width:100%\">\n\t\t\t<label for=\"urlencoder-decoded\">Decoded</label><br>\n\t\t\t<textarea id=\"urlencoder-decoded\" style=\"width:100%\" rows=\"1\" placeholder=\"https://en.wikipedia.org/wiki/Hong Kong\"></textarea><br>\n\t\t\t<label for=\"urlencoder-encoded\">Encoded</label><br>\n\t\t\t<input id=\"urlencoder-encoded\" type=\"text\" style=\"width:100%\" placeholder=\"https://en.wikipedia.org/wiki/Hong%20Kong\"><br>\n\t\t</form>\n\t\t<script>\n\t\t\t(function () {\n\t\t\t\tvar encoded = document.getElementById(\"urlencoder-encoded\");\n\t\t\t\tvar decoded = document.getElementById(\"urlencoder-decoded\");\n\t\t\t\tvar changing = false;\n\t\t\t\tencoded.oninput = function () {\n\t\t\t\t\tif (!changing) {\n\t\t\t\t\t\tchanging = true;\n\t\t\t\t\t\tdecoded.value = decodeURI(encoded.value);\n\t\t\t\t\t\tchanging = false;\n\t\t\t\t\t}\n\t\t\t\t};\n\t\t\t\tdecoded.oninput = function () {\n\t\t\t\t\tif (!changing) {\n\t\t\t\t\t\tchanging = true;\n\t\t\t\t\t\tencoded.value = encodeURI(decoded.value);\n\t\t\t\t\t\tchanging = false;\n\t\t\t\t\t}\n\t\t\t\t};\n\t\t\t})();</script>"
	},
	{
		"property_title_t": "LaTeX 預覽|Formula preview",
		"property_catalog_t": "TeX 数学公式",
		"html_s": "<script type='text/x-mathjax-config'>\n\t\t\tMathJax.Hub.Config({\n\t\t\tshowProcessingMessages: false,\n\t\t\ttex2jax: { inlineMath: [['$','$'],['\\\\(','\\\\)']] }\n\t\t\t});\n\t\t</script>\n\t\t<script type='text/javascript' src='https://cdn.jsdelivr.net/npm/mathjax@2.7.2/MathJax.js?config=TeX-MML-AM_HTMLorMML'></script>\n\n\t\t<textarea id='MathInput' cols='50' rows='5' oninput='Preview.Update()' style='margin-top:5px' placeholder=\"$x^2$\">\n\t\t</textarea>\n\n\t\t<div id='MathPreview' style='border:1px solid; padding: 3px; width:50%; margin-top:5px'></div>\n\t\t<div id='MathBuffer' style='border:1px solid; padding: 3px; width:50%; margin-top:5px; visibility:hidden; position:absolute; top:0; left: 0'></div>\n\n\t\t<script>\nvar Preview = {\n\tdelay: 150,\n\tpreview: null,\n\tbuffer: null,\n\ttimeout: null,\n\tmjRunning: false,\n\tmjPending: false,\n\toldText: null,\n\tInit: function () {\n\t\tthis.preview = document.getElementById('MathPreview');\n\t\tthis.buffer = document.getElementById('MathBuffer');\n\t},\n\tSwapBuffers: function () {\n\t\tvar buffer = this.preview, preview = this.buffer;\n\t\tthis.buffer = buffer;\n\t\tthis.preview = preview;\n\t\tbuffer.style.visibility = 'hidden';\n\t\tbuffer.style.position = 'absolute';\n\t\tpreview.style.position = '';\n\t\tpreview.style.visibility = '';\n\t},\n\tUpdate: function () {\n\t\tif (this.timeout) {\n\t\t\tclearTimeout(this.timeout)\n\t\t}\n\t\tthis.timeout = setTimeout(this.callback, this.delay);\n\t},\n\tCreatePreview: function () {\n\t\tPreview.timeout = null;\n\t\tif (this.mjPending)\n\t\t\treturn;\n\t\tvar text = document.getElementById('MathInput').value;\n\t\tif (text === this.oldtext)\n\t\t\treturn;\n\t\tif (this.mjRunning) {\n\t\t\tthis.mjPending = true;\n\t\t\tMathJax.Hub.Queue(['CreatePreview', this]);\n\t\t} else {\n\t\t\tthis.buffer.innerHTML = this.oldtext = text;\n\t\t\tthis.mjRunning = true;\n\t\t\tMathJax.Hub.Queue(\n\t\t\t\t\t['Typeset', MathJax.Hub, this.buffer],\n\t\t\t\t\t['PreviewDone', this]\n\t\t\t\t\t);\n\t\t}\n\t},\n\tPreviewDone: function () {\n\t\tthis.mjRunning = this.mjPending = false;\n\t\tthis.SwapBuffers();\n\t}\n};\nPreview.callback = MathJax.Callback(['CreatePreview', Preview]);\nPreview.callback.autoReset = true;\nPreview.Init();\n\t\t</script>"
	},
	{
		"property_title_t": "日出日落時間|Sunrise/sunset",
		"property_catalog_t": "",
		"html_s": "<form>\n\t\t\t<label for='sunrise-date'>Date(日期)</label><input id='sunrise-date' type='date' oninput='updateSunrise()'>\n\t\t\t<label for='sunrise-tz'>Timezone(時區)</label><input id='sunrise-tz' type='number' value='8' oninput='updateSunrise()'><br>\n\t\t\t<label for='sunrise-latitude'>Latitude(北緯)</label><input id='sunrise-latitude' type='number' value='22.30187' oninput='updateSunrise()'>\n\t\t\t<label for='sunrise-longitude'>Longitude(東經)</label><input id='sunrise-longitude' type='number' value='114.17340' oninput='updateSunrise()'><br>\n\t\t\t<label for='sunrise-time'>Sunrise(日出)</label><output id='sunrise-time'></output>\n\t\t\t<label for='sunset-time'>Sunset(日落)</label><output id='sunset-time'></output>\n\t\t</form>\n\t\t<script>\n\t\t\tvar updateSunrise = function () {\n\t\t\t\tvar date = document.getElementById('sunrise-date').value;\n\t\t\t\tvar latitude = parseFloat(document.getElementById('sunrise-latitude').value);\n\t\t\t\tvar longitude = parseFloat(document.getElementById('sunrise-longitude').value);\n\t\t\t\tvar tz = parseFloat(document.getElementById('sunrise-tz').value);\n\t\t\t\tvar dateFields = /([0-9]{4})-([0-9]{2})-([0-9]{2})/.exec(date);\n\t\t\t\tvar year = parseInt(dateFields[1], 10);\n\t\t\t\tvar month = parseInt(dateFields[2], 10);\n\t\t\t\tvar day = parseInt(dateFields[3], 10);\n\t\t\t\tvar N1 = Math.floor(275 * month / 9);\n\t\t\t\tvar N2 = Math.floor((month + 9) / 12);\n\t\t\t\tvar N3 = (1 + Math.floor((year - 4 * Math.floor(year / 4) + 2) / 3));\n\t\t\t\tvar N = N1 - (N2 * N3) + day - 30;\n\t\t\t\tvar lngHour = longitude / 15;\n\t\t\t\tvar calculateCosh = function (t) {\n\t\t\t\t\tvar t = N + ((6 - lngHour) / 24);\n\t\t\t\t\tvar M = (0.9856 * t) - 3.289;\n\t\t\t\t\tvar L = M + (1.916 * Math.sin(M * Math.PI / 180)) + (0.020 * Math.sin(2 * M * Math.PI / 180)) + 282.634;\n\t\t\t\t\tL = (L + 360000) % 360;\n\t\t\t\t\tvar RA = Math.atan(0.91764 * Math.tan(L * Math.PI / 180)) * 180 / Math.PI;\n\t\t\t\t\tRA = (RA + 360000) % 360;\n\t\t\t\t\tvar Lquadrant = (Math.floor(L / 90)) * 90;\n\t\t\t\t\tvar RAquadrant = (Math.floor(RA / 90)) * 90;\n\t\t\t\t\tRA += (Lquadrant - RAquadrant);\n\t\t\t\t\tRA /= 15;\n\t\t\t\t\tvar sinDec = 0.39782 * Math.sin(L * Math.PI / 180);\n\t\t\t\t\tvar cosDec = Math.cos(Math.asin(sinDec));\n\t\t\t\t\treturn [(Math.cos(90.83 * Math.PI / 180) - (sinDec * Math.sin(latitude * Math.PI / 180))) / (cosDec * Math.cos(latitude * Math.PI / 180)), RA, t];\n\t\t\t\t}\n\t\t\t\tvar formatTime = function (H, RA, t) {\n\t\t\t\t\tH /= 15;\n\t\t\t\t\tvar T = H + RA - (0.06571 * t) - 6.622;\n\t\t\t\t\tvar UT = T - lngHour;\n\t\t\t\t\tUT = (UT + 2400 + tz) % 24;\n\t\t\t\t\tvar hour = Math.floor(UT);\n\t\t\t\t\tvar minute = Math.floor((UT - hour) * 60);\n\t\t\t\t\treturn hour + ':' + minute;\n\t\t\t\t}\n\t\t\t\tvar calculateSunrise = function () {\n\t\t\t\t\tvar t = N + ((6 - lngHour) / 24);\n\t\t\t\t\tvar cosH = calculateCosh(t);\n\t\t\t\t\tif (cosH[0] > 1)\n\t\t\t\t\t\treturn '';\n\t\t\t\t\telse {\n\t\t\t\t\t\tvar H = 360 - Math.acos(cosH[0]) * 180 / Math.PI;\n\t\t\t\t\t\treturn formatTime(H, cosH[1], cosH[2]);\n\n\t\t\t\t\t}\n\t\t\t\t}\n\t\t\t\tvar calculateSunset = function () {\n\t\t\t\t\tvar t = N + ((18 - lngHour) / 24);\n\t\t\t\t\tvar cosH = calculateCosh(t);\n\t\t\t\t\tif (cosH[0] < -1)\n\t\t\t\t\t\treturn '';\n\t\t\t\t\telse {\n\t\t\t\t\t\tvar H = Math.acos(cosH[0]) * 180 / Math.PI;\n\t\t\t\t\t\treturn formatTime(H, cosH[1], cosH[2]);\n\t\t\t\t\t}\n\t\t\t\t}\n\t\t\t\tdocument.getElementById('sunrise-time').textContent = calculateSunrise();\n\t\t\t\tdocument.getElementById('sunset-time').textContent = calculateSunset();\n\t\t\t};\n\t\t\tdocument.getElementById('sunrise-date').value = new Date().toISOString().substring(0, 10);\n\t\t\tupdateSunrise();\n\t\t</script>"
	},
